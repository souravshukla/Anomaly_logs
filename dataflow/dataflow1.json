{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMPLOYEE_ID as short,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          DEPARTMENT as string,",
				"          HIRE_DATE as date,",
				"          SALARY as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 aggregate(groupBy(DEPARTMENT),",
				"     SALARY = sum(SALARY)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          QUERY_ID as string,",
				"          QUERY_START_TIME as timestamp,",
				"          USER_NAME as string,",
				"          DIRECT_OBJECTS_ACCESSED as any[],",
				"          BASE_OBJECTS_ACCESSED as any[],",
				"          OBJECTS_MODIFIED as any[],",
				"          OBJECT_MODIFIED_BY_DDL as any,",
				"          POLICIES_REFERENCED as any[],",
				"          PARENT_QUERY_ID as string,",
				"          ROOT_QUERY_ID as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     stageInsert: true) ~> sink1"
			]
		}
	}
}