{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable2",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     wildcardPaths:['snowflake/snowpark/athlete_events.csv']) ~> source1",
				"source1 select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          TM_FISCAL_YEAR as string,",
				"          MONTH_NAME as string,",
				"          MONTH as string,",
				"          PARTNER_TYPE as string,",
				"          DEALER as string,",
				"          DEALER_CODE as string,",
				"          DIVISION as string,",
				"          REGION as string,",
				"          {JC #} as string,",
				"          JOB_CARD_CREATED_DATE as string,",
				"          JC_OPEN_DAYS as string,",
				"          JOB_CARD_CLOSED_DATE as string,",
				"          SERVICE_TYPE as string,",
				"          KMS_RUN as string,",
				"          PART_NO as string,",
				"          PART_DESC as string,",
				"          CATEGORY as string,",
				"          TM_PART_INDICATOR as string,",
				"          SOLD_QTY as string,",
				"          TM_SPARES_AMOUNT as string,",
				"          SPARES_AMOUNT as string,",
				"          PPL as string,",
				"          PL as string,",
				"          LOB as string,",
				"          INVOICE_STATUS as string,",
				"          INVOICE_NUMBER as string,",
				"          INVOICE_DATE as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true) ~> sink1"
			]
		}
	}
}