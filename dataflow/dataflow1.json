{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable2_copy1",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMPLOYEE_ID as short,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          DEPARTMENT as string,",
				"          HIRE_DATE as date,",
				"          SALARY as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 aggregate(groupBy(DEPARTMENT),",
				"     SALARY = sum(SALARY)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TM_FISCAL_YEAR as string,",
				"          MONTH_NAME as string,",
				"          MONTH as string,",
				"          PARTNER_TYPE as string,",
				"          DEALER as string,",
				"          DEALER_CODE as string,",
				"          DIVISION as string,",
				"          REGION as string,",
				"          {JC #} as string,",
				"          JOB_CARD_CREATED_DATE as string,",
				"          JC_OPEN_DAYS as string,",
				"          JOB_CARD_CLOSED_DATE as string,",
				"          SERVICE_TYPE as string,",
				"          KMS_RUN as string,",
				"          PART_NO as string,",
				"          PART_DESC as string,",
				"          CATEGORY as string,",
				"          TM_PART_INDICATOR as string,",
				"          SOLD_QTY as string,",
				"          TM_SPARES_AMOUNT as string,",
				"          SPARES_AMOUNT as string,",
				"          PPL as string,",
				"          PL as string,",
				"          LOB as string,",
				"          INVOICE_STATUS as string,",
				"          INVOICE_NUMBER as string,",
				"          INVOICE_DATE as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     saveOrder: 1,",
				"     stageInsert: true) ~> sink1"
			]
		}
	}
}